<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lacrose.lc.lclacrose">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Util.MainActivity" />
        <activity
            android:name=".LoginActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:theme="@style/AppThemeFullscreen"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScanActivity"
            android:theme="@style/AppThemeFullscreen" />
        <activity
            android:name=".AboutActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/about_title"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".TermsActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/terms_of_user_title"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".WorkActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/works" />
        <activity
            android:name=".HomeActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:parentActivityName=".WorkActivity" />
        <activity
            android:name=".CorpoMoldActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/mold"
            android:parentActivityName=".ChooseMoldActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".RupturaCorpoActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/ruptura"
            android:parentActivityName=".LotesActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".RupturaBlocoActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/ruptura"
            android:parentActivityName=".LotesActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".RupturaPrismaActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/ruptura"
            android:parentActivityName=".LotesActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".RupturaPavimentoActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/ruptura"
            android:parentActivityName=".LotesActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".LotesActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/lote_label"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".RupturaCorpoListActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/list_ruptura_label"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".RupturaBlocoListActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/list_bloco_label"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".RupturaPrismasListActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/list_prismas_label"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".RupturaPavimentoListActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/list_pavimento_label"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".BlocoMoldActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/moldagem_bloco_label"
            android:windowSoftInputMode="stateHidden"
            android:parentActivityName=".ChooseMoldActivity"/>
        <activity
            android:name=".PrismaMoldActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/moldagem_prisma_label"
            android:windowSoftInputMode="stateHidden"
            android:parentActivityName=".ChooseMoldActivity"/>
        <activity
            android:name=".PavimentoMoldActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/moldagem_pavimento_label"
            android:windowSoftInputMode="stateHidden"
            android:parentActivityName=".ChooseMoldActivity"/>
        <activity
            android:name=".ChooseMoldActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/moldagem_type_label"
            android:windowSoftInputMode="stateHidden"
            android:parentActivityName=".ChooseMoldActivity" />
    </application>

</manifest>